{
    "name": "stan-vscode",
    "displayName": "stan-vscode",
    "description": "Stan language support for VS Code",
    "version": "0.0.1",
    "publisher": "ivan-bocharov",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "stan",
            "aliases": ["Stan", "stan"],
            "extensions": [".stan"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "stan",
            "scopeName": "source.stan",
            "path": "./syntaxes/stan.tmLanguage"
        }]
    }
}