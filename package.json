{
    "name": "stan-vscode",
    "displayName": "stan-vscode",
    "description": "Stan language support for VS Code",
    "version": "0.2.4",
    "publisher": "ivan-bocharov",
    "repository": "https://github.com/bocharov-ivan/stan-vscode",
    "icon": "img/stan_logo.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "stan",
                "aliases": [
                    "Stan",
                    "stan"
                ],
                "extensions": [
                    ".stan",
                    ".stanfunctions"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "stan",
                "scopeName": "source.stan",
                "path": "./syntaxes/stan.tmLanguage.json"
            }
        ],
        "snippets": [
			{
				"language": "stan",
				"path": "./snippets/snippets.json"
			}
        ]
    }
}
